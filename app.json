{
  "expo": {
    "name": "Futsal Lega",
    "slug": "fs",
    "version": "1.0.0",
    "runtimeVersion": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "icon": "./assets/icon.png",
    "backgroundColor": "#001A33",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#001A33"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.monterohidalgo.futsallega"
    },
    "android": {
      "package": "com.monterohidalgo.futsallega",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#001A33"
      },
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA"
      ]
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicación necesita acceso a la galería para subir fichas o actas de partidos."
        }
      ],
      "expo-updates"
    ],
    "owner": "monterohidalgomiguelma",
    "extra": {
      "eas": {
        "projectId": "cd24386d-84a7-4e01-8b09-8e32ecc78a26"
      }
    }
  }
}